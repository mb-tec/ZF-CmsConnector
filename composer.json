{
  "name": "mb-tec/zf-cmsconnector",
  "description": "CMS Connector for Zend Framework 3",
  "homepage": "https://github.com/mb-tec/ZF-CmsConnector",
  "type": "library",
  "license": "GPL-2.0",
  "keywords": [
    "mb-tec",
    "cms",
    "connector",
    "zf3",
    "zend",
    "framework"
  ],
  "authors": [
    {
      "name": "Matthias BÃ¼sing",
      "email": "info@mb-tec.eu",
      "homepage": "http://www.mb-tec.eu",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7.0",
    "ext-curl": "*",
    "zendframework/zend-servicemanager": "^3.2",
    "mb-tec/zf-log": "^1.1",
    "zendframework/zend-http": "^2.7",
    "zendframework/zend-uri": "^2.5",
    "zendframework/zend-filter": "^2.7",
    "zendframework/zend-cache": "^2.7"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.7",
    "zendframework/zend-coding-standard": "^1.0",
    "phpunit/phpunit": "^5.7",
    "phpmd/phpmd": "^2.5",
    "sebastian/phpcpd": "^2.0",
    "phploc/phploc": "^3.0"
  },
  "suggest": {
    "zendframework/zend-modulemanager": "To use it as a Zend Framework module"
  },
  "autoload": {
    "psr-4": {
      "MBtec\\CmsConnector\\": "src/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MBtec\\CmsConnector\\Test\\": "test/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "phpcpd": "phpcpd",
    "phpmd": "phpmd",
    "phploc": "phploc src",
    "test": "phpunit"
  }
}
